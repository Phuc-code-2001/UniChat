@{
    ViewData["Title"] = "Create";
}

@model RoomDeadLine
<div class="container">

    <h2>Create new deadline for RoomId: @Model.RoomId</h2>

    <form asp-action="Create" method="post" class="d-flex flex-column" style="gap: .5rem;">
        <textarea asp-for="Content" cols="30" rows="10"></textarea>
        <input type="datetime" asp-for="ExpirationTime">
        <input type="submit" value="Create">
    </form>

</div>